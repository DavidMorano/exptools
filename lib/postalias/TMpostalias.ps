()l
(    # lookup - lookup of upas aliases and alias lists)l
(    # ----------------------------------------------------------------------)l
(    # Written by Gines Milara and modified by Alex Damman.)l
(    )l
(    LIST=/usr/lib/upas/list)l
(    PATH=$PATH:/usr/lib/upas:/usr/lbin:)l
(    )l
(    cd $LIST)l
(    )l
(    awk ' {)l
()0(path = $2;)8 L
()0(getline;)8 L
()0(while \(index\($1,":"\) > 0\) {)8 L
()0(if \($1 == "Subject:"\))12 L
()0(list = substr\($0, length\($1\) + 1\);)20 L
()0(getline;)12 L
()0(})8 L
()0(while \(1\) {)8 L
()0(list = list " " $0;)12 L
()0(if \(getline == 0\))12 L
()0(break;)20 L
()0(})8 L
(    } END {print path list} ' - | sed "s/ `uname -n`!/ /g" | \()l
(    read path args)l
(    # echo "Subject: $args" # doesn't seem to work)l
(    for token in $args)l
(    do  if)0([ "$token" = "aliases" ])16 L
()0(then    echo "# aliases\\n`/bin/ls`")8 L
()0(elif    [ ! -f "$token" -a ! -f "../Olist/$token" ])8 L
()0(then    echo "# $token: No such alias")8 L
()0(else    echo "# $token")8 L
()0(for alias in `translate $token`)12 L
()0(do  if expr "$alias" : '.*[!]' >/dev/null||test -f "$alias")12 L
()0(then)20(echo " $alias")32 L
()0(else)20(eval post -w -q multi= "$alias" 2>&1)32 L
()0(fi)20 L
()0(done | sort)12 L
()0(fi)8 L
(    done | sed 's/^ //' |)0(/bin/rmail $path)32 L
(    \))l
