#  @(#)  SID: 1.2;  module: @error@;  last changed: 3/17/91  11:18:45

case $1 in
    1) echo "No modules identified in the \"Regress\" command -- aborting!\n" ;;
    2) echo "Incorrect number of parameters.\n" ;;
    3) echo "Unable to find  \"$2\"  file.\n" ;;
    5) echo "No parameters encountered.\n" ;;
    6) echo "Can't figure out what to do with  \"$2\"\n" ;;
    7) echo "Unable to read  \"$2\"  file.\n" ;;
    *) echo "Error occurred in @ command.\n" ;;
esac
